{
  "identifier": "shell:allow-execute",
  "description": "允许执行指定的 shell 命令",
  "permissions": [
    "shell:default"
  ],
  "allow": [
    {
      "name": "exec-sh",
      "cmd": "sh",
      "args": [
        "-c",
        {
          "validator": "\\S+"
        }
      ],
      "sidecar": false
    }
  ]
}
